  },
  "scripts": {
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "watch-node": "parcel watch ./src/testing/node/index.ts --out-dir ./dist/testing/node --target node",
    "watch-browser": "parcel watch ./src/testing/node/static/index.html --out-dir ./dist/testing/node/static/ --target browser",
    "build-node": "parcel build ./src/testing/node/index.ts --out-dir ./dist/testing/node --target node",
    "build-browser": "parcel build ./src/testing/node/static/index.html --out-dir ./dist/testing/node/static/ --target browser",
    "start": "node ./dist/testing/node/",
    "debug": "nodemon --inspect ./dist/testing/node/ --ignore */testing/node/static/client --verbose",
    "build-docs": "npx --no-install typedoc ./src/ --exclude node_modules --ignoreCompilerErrors --target ES6",
    "read-docs": "static ./docs/",
    "dev": "./devops/dev.sh"
  },
  "keywords": [],
  "author": "Alexander V. Pavlovcik <nvcrm@pavlovcik.com>",

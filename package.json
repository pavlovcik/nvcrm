    "browserslist": "^4.7.3",
    "caniuse-lite": "^1.0.30001012",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "parcel-bundler": "^1.12.4",
    "typedoc": "^0.15.3",
    "typescript": "^3.8.3"
